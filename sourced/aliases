alias nc-support mirko, andreas, alexander

# ~/bin/muttquery.py ncomputing nc-company | awk '/@/{c=$2; for(i=3; i<=NF; i++) c=c" "$i; print c" <"$1">"}' | while read c; do n=${c%% *}; echo alias $n:l \"$c\"; done >> aliases
